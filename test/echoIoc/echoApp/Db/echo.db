# Pressure
record(ai, "$(P):Pressure-Mon") {
    field(SCAN, "1 second")
    field(DESC, "Measured pressure")
    field(DTYP, "stream")
    field(EGU,  "mBar")
    field(INP,  "@echo.proto pressureMon(131,1) $(PORT)")
}

