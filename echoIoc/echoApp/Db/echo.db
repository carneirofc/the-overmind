record(stringout, "$(P):Write"){
    field(DTYP, "stream")
    field(OUT,  "@echo.proto write $(PORT) $(A)")
}

record(stringin, "$(P):Read"){
    field(DTYP, "stream")
    field(INP,  "@echo.proto read $(PORT) $(A)")
}

record(stringin, "$(P):Query1"){
    field(DTYP, "stream")
    field(INP,  "@echo.proto query(1) $(PORT) $(A)")
    field(SCAN, "1 second")
}
