record(stringout, "$(P):Write"){
    field(DTYP, "stream")
    field(OUT,  "@echo.proto write $(PORT) $(A)")
}

record(stringin, "$(P):Read"){
    field(DTYP, "stream")
    field(INP,  "@echo.proto read $(PORT) $(A)")
}

record(stringin, "$(P):Query"){
    field(DTYP, "stream")
    field(INP,  "@echo.proto query $(PORT) $(A)")
    field(SCAN, "1 second")
}

